*.font: xft:Fira Code Light:size=10,xft:Hack:size=10,xft:PowerlineSymbols:size=10

rofi.font: Fira Sans 8

Xft.dpi:                        96
Xft.antialias:                  1
Xft.autohint:                   1
Xft.hinting:                    1
Xft.hintstyle:                  hintslight
Xft.lcdfilter:                  lcdlight
Xft.rgba:                       rgb

URxvt*termName:                 rxvt-256color
URxvt.depth:                    32
URxvt*lineSpace:                1
URxvt.letterSpace:              -1
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt*geometry:                 65x17
URxvt.scrollBar:                false
URxvt*loginShell:               true
URxvt.internalBorder:           25
URxvt.perl-ext:	 		default,matcher
URxvt.url-launcher:             firefox
URxvt.matcher.button: 		1

! X colors.
! Generated by 'wal'
*foreground:        #e7e1eb
*background:        #04132C
*.foreground:       #e7e1eb
*.background:       #04132C
emacs*foreground:   #e7e1eb
emacs*background:   #04132C
URxvt*foreground:   #e7e1eb
XTerm*foreground:   #e7e1eb
UXTerm*foreground:  #e7e1eb
URxvt*background:   [85]#04132C
XTerm*background:   #04132C
UXTerm*background:  #04132C
URxvt*cursorColor:  #e7e1eb
XTerm*cursorColor:  #e7e1eb
